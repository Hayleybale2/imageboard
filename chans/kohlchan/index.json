{
	"id": "kohlchan",
	"domain": "kohlchan.net",
	"engine": "lynxchan",
	"thumbnailSize": 200,
	"defaultAuthorName": {
		"*": "Bernd",
		"ru": "Бернд"
	},
  "capcode": {
    "Administrator": {
    	"role": "administrator"
    },
    "Global Moderator": {
    	"role": "moderator"
    },
    "Board Owner": {
    	"role": "moderator",
    	"scope": "board"
    }
  },
	"boardUrl": "/{boardId}",
  "threadUrl": "/{boardId}/res/{threadId}.html",
  "commentUrl": "/{boardId}/res/{threadId}.html#{commentId}",
	"api": {
		"getThread": {
			"method": "GET",
			"url": "/{boardId}/res/{threadId}.json",
			"urlParameters": [{
				"name": "boardId",
				"input": {
					"name": "boardId"
				}
			}, {
				"name": "threadId",
				"input": {
					"name": "threadId"
				}
			}]
		},
		"getThreads": {
			"method": "GET",
			"url": "/{boardId}/catalog.json",
			"urlParameters": [{
				"name": "boardId",
				"input": {
					"name": "boardId"
				}
			}]
		},
		"getThreadsWithLatestCommentsPage": {
			"method": "GET",
			"url": "/{boardId}/{page}.json",
			"urlParameters": [{
				"name": "boardId",
				"input": {
					"name": "boardId"
				}
			}, {
				"name": "page",
				"input": {
					"name": "page"
				}
			}]
		},
		"getBoardsPage": {
			"method": "GET",
			"url": "/boards.js?json=1&page={page}",
			"urlParameters": [{
				"name": "page",
				"input": {
					"name": "page"
				}
			}]
		}
	},
	"boardCategoryTitleById": {
		"vip": "VIP",
		"allgemein": "Allgemein",
		"weltweit": "Weltweit",
		"sonstiges": "Sonstiges",
		"*": "Andere"
	}
}