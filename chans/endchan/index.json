{
	"id": "endchan",
	"domain": "endchan.net",
	"engine": "lynxchan",
	"thumbnailSize": 255,
	"defaultAuthorName": {
		"*": "Anonymous",
		"ausneets": "NEET",
		"kc": "Bernd"
	},
	"boardUrl": "/{boardId}",
	"threadUrl": "/{boardId}/res/{threadId}.html",
	"commentUrl": "/{boardId}/res/{threadId}.html#{commentId}",
	"api": {
		"getThread": {
			"method": "GET",
			"url": "/{boardId}/res/{threadId}.json",
			"urlParameters": [{
				"name": "boardId",
				"input": {
					"name": "boardId"
				}
			}, {
				"name": "threadId",
				"input": {
					"name": "threadId"
				}
			}]
		},
		"getThreads": {
			"method": "GET",
			"url": "/{boardId}/catalog.json",
			"urlParameters": [{
				"name": "boardId",
				"input": {
					"name": "boardId"
				}
			}]
		},
		"getBoards": {
			"method": "GET",
			"url": "/index.json"
		}
	}
}