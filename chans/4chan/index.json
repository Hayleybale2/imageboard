{
	"id": "4chan",
	"engine": "4chan",
	"domain": "4chan.org",
	"domainByBoard": {
		"*": "boards.4channel.org",
		"notSafeForWork": "boards.4chan.org"
	},
	"defaultAuthorName": "Anonymous",
	"capcode": {
		"admin": "administrator",
		"admin_highlight": "administrator",
		"founder": "administrator",
		"developer": "administrator",
		"mod": "moderator",
		"manager": "moderator"
	},
	"attachmentUrl": "https://i.4cdn.org/{boardId}/{name}{ext}",
	"attachmentThumbnailUrl": "https://i.4cdn.org/{boardId}/{name}s.jpg",
	"fileAttachmentUrl": "https://i.4cdn.org/{boardId}/{originalName}{ext}",
	"boardUrl": "/{boardId}",
	"threadUrl": "/{boardId}/thread/{threadId}",
	"commentUrl": "/{boardId}/thread/{threadId}#p{commentId}",
	"authorBadgeUrl": "https://s.4cdn.org/image/flags/{boardId}/{flagId}.gif",
	"authResetUrl": "https://www.4chan.org/pass?reset",
	"incrementalThreadUpdateStartsAtCommentsCount": 101,
	"api": {
		"getThread": "https://a.4cdn.org/{boardId}/thread/{threadId}.json",
		"getThreadIncremental": "https://a.4cdn.org/{boardId}/thread/{threadId}-tail.json",
		"getThreads": "https://a.4cdn.org/{boardId}/catalog.json",
		"getThreadsWithLatestComments": "https://a.4cdn.org/{boardId}/catalog.json",
		"getBoards": "https://a.4cdn.org/boards.json",
		"post": {
			"method": "POST",
			"url": "https://sys.4chan.org/{boardId}/post",
			"urlParameters": [{
				"name": "boardId",
				"input": {
					"name": "boardId"
				}
			}],
			"responseType": "text/html",
			"parameters": [{
				"name": "mode",
				"defaultValue": "regist"
			}, {
				"name": "resto",
				"input": {
					"name": "threadId"
				}
			}, {
				"name": "name",
				"input": {
					"name": "authorName"
				}
			}, {
				"name": "email",
				"input": {
					"name": "authorEmail"
				}
			}, {
				"name": "pwd",
				"input": {
					"name": "accessToken"
				}
			}, {
				"name": "sub",
				"input": {
					"name": "title"
				}
			}, {
				"name": "com",
				"input": {
					"name": "content"
				}
			}, {
				"name": "upfile",
				"input": {
					"name": "attachments",
					"index": 0
				}
			}, {
				"name": "spoiler",
				"input": {
					"name": "attachmentSpoiler"
				}
			}, {
				"name": "filetag",
				"input": {
					"name": "attachmentFileTag"
				}
			}, {
				"name": "textonly",
				"input": {
					"name": "textOnly"
				}
			}, {
				"name": "g-recaptcha-response",
				"input": {
					"name": "captchaSolution"
				}
			}]
		},
		"report": {
			"method": "POST",
			"url": "https://sys.4chan.org/{boardId}/imgboard.php",
			"urlParameters": [{
				"name": "boardId",
				"input": {
					"name": "boardId"
				}
			}],
			"responseType": "text/html",
			"parameters": [{
				"name": "mode",
				"defaultValue": "report"
			}, {
				"name": "no",
				"input": {
					"name": "commentId"
				}
			}, {
				"name": "g-recaptcha-response",
				"input": {
					"name": "captchaSolution"
				}
			}]
		},
		"logIn": {
			"method": "POST",
			"url": "https://sys.4chan.org/auth",
			"responseType": "text/html",
			"parameters": [{
				"name": "act",
				"input": {
					"name": "do_login"
				}
			}, {
				"name": "id",
				"input": {
					"name": "authToken"
				}
			}, {
				"name": "pin",
				"input": {
					"name": "authTokenPassword"
				}
			}, {
				"name": "long_login",
				"defaultValue": "yes"
			}]
		},
		"logOut": {
			"method": "POST",
			"url": "https://sys.4chan.org/auth",
			"responseType": "text/html"
		}
	}
}